  "homepage": "https://origen.chat",
  "scripts": {
    "build": "yarn build-lib && yarn build-types",
    "build-lib": "cross-env NODE_ENV=production rollup --config rollup.config.js",
    "build-types": "tsc --emitDeclarationOnly",
    "prebuild": "yarn clean",
    "storybook": "start-storybook --host 0.0.0.0 --port 5001 --config-dir .storybook --https --ssl-cert ./keys/cert.pem --ssl-key ./keys/key.pem",
    "make-keys": "mkcert localhost 127.0.0.1 ::1 0.0.0.0",
    "premake-keys": "rimraf keys",
    "postmake-keys": "mkdirp keys && mv localhost+3.pem keys/cert.pem && mv localhost+3-key.pem keys/key.pem",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "yarn test --onlyChanged --watch",
    "test:ci": "yarn test --coverage",
    "typecheck": "yarn tsc --noEmit",
    "lint:check": "eslint \"src/**/*.{js,ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,ts,tsx}\"",
    "clean": "rimraf dist",
    "coverage": "codecov",
    "commit": "commit",
    "semantic-release": "semantic-release"
  },
